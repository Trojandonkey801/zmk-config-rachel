/ {
    combos {
        compatible = "zmk,combos";

        u_combo_base {
            bindings = <&to 1>;
            key-positions = <10 11>;
        };

        u_combo_esc {
            bindings = <&kp ESC>;
            key-positions = <24 35>;
        };

        u_combo_extra {
            bindings = <&to 0>;
            key-positions = <23 22>;
        };

        u_combo_zoom {
            bindings = <&u_macro_app_zoom>;
            key-positions = <6 7>;
        };

        u_combo_zoom_mic {
            bindings = <&u_macro_app_zoom_mic>;
            key-positions = <18 19>;
        };

        u_combo_zoom_end {
            bindings = <&u_macro_app_zoom_end>;
            key-positions = <31 30>;
        };

        u_combo_angle_open {
            bindings = <&kp LT>;
            key-positions = <32 31>;
        };

        u_combo_angle_close {
            bindings = <&kp GT>;
            key-positions = <32 33>;
        };

        u_combo_playpause {
            bindings = <&kp C_PLAY_PAUSE>;
            key-positions = <29 30>;
        };

        u_combo_volup {
            bindings = <&kp C_VOL_UP>;
            key-positions = <10 22>;
        };

        u_combo_voldown {
            bindings = <&kp C_VOL_DN>;
            key-positions = <22 34>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        EXTRA {
            label = "Extra";
            bindings = <
&u_mt ESC GRAVE  &kp Q      &kp W  &kp E             &kp R             &kp T          &kp Y        &kp U              &kp I              &kp O           &kp P           &kp BACKSPACE
&u_caps_word     &kp A      &kp S  &u_mt LEFT_ALT D  &u_mt LEFT_GUI F  &kp G          &kp H        &u_mt RIGHT_GUI J  &u_mt RIGHT_ALT K  &kp L           &u_mt AT EQUAL  &u_mt AMPS SQT
&sk LEFT_SHIFT   &u_lt 6 Z  &kp X  &kp C             &kp V             &kp B          &kp N        &kp M              &u_mt BSLH COMMA   &u_mt SEMI DOT  &mt EXCL SLASH  &sk RCTRL
                                   &sk LEFT_GUI      &u_lt 5 SPACE     &u_lt 5 TAB    &u_lt 2 RET  &u_lt 3 DEL        &u_lt 6 COLON
            >;
        };

        BASE {
            label = "Base";
            bindings = <
&u_mt ESC GRAVE  &kp Q      &kp W  &kp F             &kp P          &kp B          &kp J        &kp L         &kp U              &kp Y           &mt AT EQUAL      &kp BACKSPACE
&u_caps_word     &kp A      &kp R  &u_mt LEFT_ALT S  &kp T          &kp G          &kp M        &kp N         &u_mt RIGHT_ALT E  &kp I           &kp O             &mt AMPS SINGLE_QUOTE
&sk LEFT_SHIFT   &u_lt 6 Z  &kp X  &kp C             &kp D          &kp V          &kp K        &kp H         &u_mt BSLH COMMA   &u_mt SEMI DOT  &u_mt EXCL SLASH  &sk RIGHT_CONTROL
                                   &sk LEFT_GUI      &u_lt 5 SPACE  &u_lt 5 TAB    &u_lt 2 RET  &u_lt 3 BSPC  &u_lt 6 DEL
            >;
        };

        SYM {
            label = "Sym";
            bindings = <
&none  &kp LBRC   &kp AMPS  &kp ASTRK          &kp BSLH                &kp RBRC     &u_macro_curly_brackets  &u_to_U_BASE       &u_to_U_EXTRA  &none              &bootloader      &kp DEL
&none  &kp COLON  &kp DLLR  &kp PRCNT          &kp CARET               &kp PLUS     &u_macro_sq_brackets     &kp RIGHT_COMMAND  &kp RIGHT_ALT  &kp RIGHT_CONTROL  &kp RIGHT_SHIFT  &none
&none  &kp TILDE  &kp EXCL  &kp AT             &kp HASH                &kp PIPE     &u_macro_single_quotes   &none              &none          &kp RALT           &none            &none
                            &u_macro_brackets  &u_macro_double_quotes  &kp UNDER    &none                    &none              &none
            >;
        };

        NUM {
            label = "Num";
            bindings = <
&none  &kp LBKT   &kp N7  &kp N8   &kp N9  &kp RBKT     &none  &u_to_U_BASE   &u_to_U_EXTRA  &none              &bootloader      &kp DEL
&none  &kp SEMI   &kp N4  &kp N5   &kp N6  &kp EQUAL    &none  &kp RIGHT_GUI  &kp RIGHT_ALT  &kp RIGHT_CONTROL  &kp RIGHT_SHIFT  &none
&none  &kp GRAVE  &kp N1  &kp N2   &kp N3  &kp BSLH     &none  &u_to_U_NUM    &u_to_U_NAV    &kp RALT           &none            &none
                          &kp DOT  &kp N0  &kp MINUS    &none  &none          &none
            >;
        };

        NAV {
            label = "Nav";
            bindings = <
&none  &bootloader     &none             &kp LA(UP_ARROW)  &u_to_U_BASE   &none    &none     &none        &kp UP     &none             &none             &none
&none  &none           &kp LA(LEFT)      &kp LA(DOWN)      &kp LA(RIGHT)  &none    &kp HOME  &kp LEFT     &kp DOWN   &kp RIGHT         &kp RA(RG(UP))    &none
&none  &kp LEFT_SHIFT  &kp LEFT_CONTROL  &kp LEFT_ALT      &kp LEFT_GUI   &none    &kp INS   &kp PAGE_UP  &kp PG_DN  &kp RA(RG(LEFT))  &kp RA(RG(DOWN))  &kp RA(RG(RIGHT))
                                         &none             &none          &none    &kp RET   &kp BSPC     &kp DEL
            >;
        };

        MEDIA {
            label = "Media";
            bindings = <
&none  &bootloader     &none             &u_to_U_EXTRA  &u_to_U_BASE   &none    &u_out_tog         &none        &kp C_VOL_UP  &none        &none        &none
&none  &kp LEFT_SHIFT  &kp LEFT_CONTROL  &kp LEFT_ALT   &kp LEFT_GUI   &none    &ext_power EP_TOG  &kp C_PREV   &kp C_VOL_DN  &kp C_NEXT   &none        &none
&none  &none           &kp RALT          &u_to_U_FUN    &u_to_U_MEDIA  &none    &u_bt_sel_0        &u_bt_sel_1  &u_bt_sel_2   &u_bt_sel_3  &u_bt_sel_4  &none
                                         &none          &none          &none    &kp C_STOP         &kp C_PP     &kp C_MUTE
            >;
        };

        FUN {
            label = "Fun";
            bindings = <
&u_macro_string_work_email           &kp F12  &kp F7  &kp F8     &kp F9     &u_macro_select_all_cut     &u_macro_screenshot      &u_to_U_BASE   &u_to_U_EXTRA  &none              &bootloader      &u_macro_screenshot_clipboard
&u_macro_string_lookforrachel_email  &kp F11  &kp F4  &kp F5     &kp F6     &u_macro_select_all_copy    &u_macro_screenshot_all  &kp RIGHT_GUI  &kp RIGHT_ALT  &kp RIGHT_CONTROL  &kp RIGHT_SHIFT  &u_macro_screenshot_clipb_all
&u_macro_string_ctask                &kp F10  &kp F1  &kp F2     &kp F3     &kp PAUSE_BREAK             &none                    &u_to_U_FUN    &u_to_U_MEDIA  &kp RALT           &none            &none
                                                      &kp K_APP  &kp SPACE  &kp TAB                     &none                    &none          &none
            >;
        };
    };
};

